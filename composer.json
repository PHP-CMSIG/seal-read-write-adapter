{
    "name": "schranz-search/seal-read-write-adapter",
    "description": "An adapter to support to split read and write operations for the schranz-search/seal package.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "schranz-search",
        "schranz-search-adapter",
        "seal-adapter",
        "search-client"
    ],
    "autoload": {
        "psr-4": {
            "Schranz\\Search\\SEAL\\Adapter\\ReadWrite\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Schranz\\Search\\SEAL\\Adapter\\ReadWrite\\Tests\\": "Tests"
        }
    },
    "authors": [
        {
            "name": "Alexander Schranz",
            "email": "alexander@sulu.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "schranz-search/seal": "^0.1",
        "psr/container": "^1.0 || ^2.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./../*",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev"
}
